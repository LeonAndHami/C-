<Window x:Class="AssistantUpdater.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="更新" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" SizeToContent="WidthAndHeight" MinWidth="320"
        Icon="./ADC2.png" ShowInTaskbar="False">
  <DockPanel Margin="5,6,6,7">
    <TextBlock Text="应用更新" DockPanel.Dock="Top" />
    <ProgressBar Name="progress" Height="25" Margin="5,6,6,7" DockPanel.Dock="Top" IsIndeterminate="True" />
    <TextBlock Name="tips" DockPanel.Dock="Top" Margin="5,3,0,8" />
  </DockPanel>
</Window>
